{
  "name": "webforge/project-stack",
  "description": "Full stack for webforge components to write a webapplication or website.",
  "license": "MIT",
  "authors": [{
    "name": "Philipp Scheit",
    "email": "p.scheit@ps-webforge.com"
  }],
  "minimum-stability": "dev",
  "autoload": {
    "psr-0": {
      "Webforge\\ProjectStack": ["lib/", "tests/"],
      "Webforge\\Code\\Test": "D:\\www\\webforge-testplate\\lib"
    }
  },
  "require": {
    "webforge/webforge": "1.9.*@dev",
    "webforge/doctrine": "1.2.*@dev",
    "webforge/view": "1.0.*@dev",
    "webforge/config": "1.0.*@stable",
    "webforge/serializer": "1.0.*@dev",

    "symfony/framework-bundle": "2.4.*@stable",
    "symfony/http-kernel": "2.4.*@stable",
    "symfony/dependency-injection": "2.4.*@stable",
    "symfony/class-loader": "2.4.*@stable",
    "symfony/security-csrf": "2.4.*",
    "symfony/validator": "2.4.*@stable",
    "symfony/finder": "2.4.*",

    "sensio/generator-bundle": "2.3.*"
  },
  "suggest": {
    "symfony/security-bundle": "to secure your application"
  },
  "require-dev": {
    "webforge/testplate": "1.*",
    "symfony/security-bundle": "2.4.*@stable"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}